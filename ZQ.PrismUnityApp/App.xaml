<Application x:Class="ZQ.PrismUnityApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:converter="clr-namespace:ZQ.PrismUnityApp.Converters"
             xmlns:local="clr-namespace:ZQ.PrismUnityApp">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <sys:String x:Key="MainRegion">MainRegion</sys:String>
                    <converter:ViewQueueConverter x:Key="ViewQueueConverter"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
